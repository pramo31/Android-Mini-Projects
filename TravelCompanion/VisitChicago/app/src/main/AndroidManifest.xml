<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uic.cs478.pramodh.visitchicago">

    <permission
        android:name="edu.uic.cs478.sp2020.project3"
        android:description="@string/broadcast_permission_desc"
        android:label="@string/broadcast_permission_label"
        android:protectionLevel="dangerous" />

    <uses-permission android:name="edu.uic.cs478.sp2020.project3" />

    <application
